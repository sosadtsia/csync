{
  "google_drive": {
    "credentials_path": "credentials.json",
    "token_path": "token.json",
    "folder_id": "",
    "scopes": [
      "https://www.googleapis.com/auth/drive.file"
    ],
    "metadata": {
      "source": "csync",
      "version": "1.0.0"
    }
  },
  "pcloud": {
    "username": "your-pcloud-email@example.com",
    "password": "your-pcloud-password",
    "api_host": "https://api.pcloud.com",
    "folder_id": ""
  },
  "general": {
    "max_concurrency": 5,
    "retry_attempts": 3,
    "chunk_size_bytes": 8388608,
    "ignore_patterns": [
      ".git/",
      ".DS_Store",
      "Thumbs.db",
      "*.tmp",
      "*.temp",
      "node_modules/",
      ".env"
    ],
    "include_patterns": [],
    "daemon_mode": false,
    "sync_interval": "5m",
    "watch_mode": false,
    "pid_file": "csync.pid",
    "log_file": "csync.log"
  }
}
